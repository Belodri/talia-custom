{
  "id": "talia-custom",
  "title": "Talia-Custom",
  "version": "1.0.0",
  "compatibility": {
    "minimum": "11",
    "verified": "11"
  },
  "esmodules": [
    "scripts/module.mjs"
  ],
  "socket": true,
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "compatibility": {}
      }
    ],
    "requires": [
      {
        "id": "socketlib",
        "type": "module",
        "compatibility": {}
      }
    ]
  },
  "styles": [
    "styles/contraptionsCraftingUi.css",
    "styles/alchemyBrewingUi.css",
    "styles/settlement.css"
  ],
  "description": "A module to handle anything custom made for the Talia campaign.",
  "packs": [
    {
      "name": "customItems",
      "label": "Custom Items",
      "path": "packs/customItems",
      "type": "Item",
      "system": "dnd5e",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "name": "rules",
      "label": "Rules",
      "path": "packs/rules",
      "type": "JournalEntry",
      "system": "dnd5e",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "name": "rollable-tables",
      "label": "Rollable Tables",
      "path": "packs/rollable-tables",
      "type": "RollTable",
      "system": "dnd5e",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "name": "taliaMacros",
      "label": "Talia Macros",
      "path": "packs/taliaMacros",
      "type": "Macro",
      "system": "dnd5e",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "name": "talia-actors",
      "label": "Talia Actors",
      "path": "packs/talia-actors",
      "type": "Actor",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      },
      "system": "dnd5e"
    }
  ],
  "flags": {
    "hotReload": {
      "extensions": [
        "css",
        "hbs"
      ],
      "paths": [
        "templates",
        "styles"
      ]
    }
  }
}